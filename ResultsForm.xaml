<Window x:Class="Snake.ResultsForm"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Results" Height="412" Width="300" ResizeMode="NoResize">
    <Grid Width="277">
        <Grid Margin="44,43,32,44">
            <ScrollViewer>
                <TextBlock TextAlignment="Center" Name="txtResults" Width="Auto" Height="Auto" ScrollViewer.CanContentScroll="True" ScrollViewer.VerticalScrollBarVisibility="Auto"></TextBlock>
            </ScrollViewer>
        </Grid>
        <Label Height="34" Margin="97,0,36,0" Name="lblResultsTxt" VerticalAlignment="Top" FlowDirection="LeftToRight" FontWeight="Bold" FontSize="20">Results</Label>
        <Button Height="23" Margin="88,0,87,11" Name="btnClose" VerticalAlignment="Bottom" Click="btnClose_Click" TabIndex="1" IsTabStop="False" IsDefault="True" IsCancel="True">Close</Button>
    </Grid>
</Window>
